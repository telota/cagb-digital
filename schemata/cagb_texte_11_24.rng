                    </attribute>
                    <attribute name="type">
                        <value>editorial</value>
                     </attribute>
                    <ref name="inlineElements"/>
                </element>
            </choice>
        </element>
    </define>
    <define name="bibl">
        <element name="bibl">
            <attribute name="sameAs"/>
            <optional>
                <attribute name="type" />
            </optional>
            <text />
        </element>
    </define>
    <define name="del">
        <!-- <element name="del"> -->
            <optional><attribute name="resp"/></optional>
            <attribute name="rendition"/>
            <ref name="inlineElements"/>
        <!-- </element> -->
    </define>
    <define name="damage">
        <element name="damage">
            <empty></empty>
        </element>
    </define>
    <define name="choice_sic_corr">
        <!-- <element name="choice"> -->
            <element name="sic">
                <choice>
                    <text></text>
                    <element name="figure">
                        <choice>
                            <element name="app">
                                <element name="lem">
                                    <attribute name="resp"></attribute>
                                    <attribute name="cert">
                                        <choice>
                                            <value>high</value>
                                            <value>medium</value>
                                            <value>low</value>
                                        </choice>
                                    </attribute>
                                    <ref name="syllogismus-diagramm"></ref>
                                </element>
                                <element name="rdg">
                                    <attribute name="wit"></attribute>
                                    <ref name="syllogismus-diagramm"></ref>
                                </element>
                            </element>
                            <ref name="syllogismus-diagramm"></ref>
                        </choice>
                    </element>
                </choice>
            </element>
            <element name="corr">
                <optional>
                    <attribute name="cert"></attribute>
                </optional>
                <optional>
                    <attribute name="type" />
                </optional>
                <choice>
                    <text></text>
                    <element name="figure">
                        <choice>
                            <element name="app">
                                <element name="lem">
                                    <attribute name="resp"></attribute>
                                    <attribute name="cert">
                                        <choice>
                                            <value>high</value>
                                            <value>medium</value>
                                            <value>low</value>
                                        </choice>
                                    </attribute>
                                    <ref name="syllogismus-diagramm"></ref>
                                </element>
                                <element name="rdg">
                                    <attribute name="wit"></attribute>
                                    <ref name="syllogismus-diagramm"></ref>
                                </element>
                            </element>
                            <ref name="syllogismus-diagramm"></ref>
                        </choice>
                    </element>
                </choice>
            </element>
        <!-- </element> -->
    </define>
    <define name="choice_orig_reg">
        <!-- <element name="choice"> -->
            <element name="orig">
                <text />
            </element>
            <element name="reg">
                <text />
            </element>
        <!-- </element> -->
    </define>
    <define name="choice_abbr_expan">
        <!-- <element name="choice"> -->
            <element name="abbr">
                <text></text>
            </element>
            <element name="expan">
                <interleave>
                    <zeroOrMore>
                        <element name="ex">
                            <text></text>
                        </element>
                    </zeroOrMore>
                    <text></text>
                </interleave>
            </element>
        <!-- </element> -->
    </define>
    <define name="del_gap">
        <!-- <element name="del"> -->
            <element name="gap">
                <attribute name="quantity"></attribute>
                <attribute name="unit"></attribute>
                <ref name="inlineElements"></ref>
            </element>
        <!-- </element> -->
    </define>
    <define name="ex">
        <element name="ex">
            <text></text>
        </element>
    </define>
    <define name="g">
        <element name="g">
            <attribute name="ref">
                <value>#typoHyphen</value>
            </attribute>
        </element>
    </define>
    <define name="gap">
        <element name="gap">
            <optional>
                <attribute name="reason" />
            </optional>
            <attribute name="quantity"></attribute>
            <attribute name="unit"></attribute>
        </element>
    </define>
    <define name="hi">
        <element name="hi">
            <attribute name="rendition"/>
            <ref name="inlineElements"></ref>
        </element>
    </define>
    <define name="index">
        <element name="index">
            <attribute name="spanTo" />
            <attribute name="corresp" />
            <attribute name="indexName" />
            <element name="term">
                <text />
            </element>
        </element>
    </define>
    <define name="lb">
        <element name="lb">
            <optional>
                <attribute name="ed" />
            </optional>
            <optional>
                <attribute name="type"></attribute>
            </optional>
            <optional>
                <attribute name="n"/>
            </optional>
            <optional>
                <attribute name="break" />
            </optional>
            <empty/>
        </element>
    </define>
    <define name="note">
        <element name="note">
            <choice>
                <group><!-- Autorenanmerkungen -->
                    <attribute name="place"></attribute>
                    <attribute name="resp"></attribute>
                    <ref name="inlineElements"></ref>
                </group>
                <group><!-- Herausgeberanmerkungen -->
                    <text></text>
                </group>
            </choice>
        </element>
    </define>
    <define name="orgName">
        <element name="orgName">
            <ref name="inlineElements"></ref>
        </element>
    </define>
    <define name="pb">
        <element name="pb">
            <optional><attribute name="ed"/></optional>
            <optional> <attribute name="facs"/> </optional>
            <attribute name="n"/>
            <empty/>
        </element>
    </define>
    <define name="persName">
        <element name="persName">
            <optional>
                <attribute name="key"/>
            </optional>
            <text></text>
        </element>
    </define>
    <define name="placeName">
        <element name="placeName">
            <ref name="inlineElements"></ref>
        </element>
    </define>
    <define name="quote">
        <element name="quote">
            <!--attribute name="xml:id">
                <data type="ID"></data>
            </attribute-->
            <optional>
                <attribute name="type">
                    <choice>
                        <value>Explizit</value>
                        <value>Zitation</value>
                        <value>Simile</value>
                        <value>Paraphrasis</value>
                        <value>witText</value>
                    </choice>
                </attribute>
            </optional>
            <optional>
                <attribute name="corresp" />
            </optional>
            <optional>
                <attribute name="source"></attribute>
            </optional>
            <optional>
                <attribute name="xml:id">
                    <data type="ID"/>
                </attribute>
            </optional>
            <optional>
                <attribute name="next" />
            </optional>
            <optional>
                <attribute name="prev" />
            </optional>
            <ref name="inlineElements"/>
            <optional>
                <element name="ref">
                    <attribute name="decls">
                        <choice>
                            <value>#Bekker</value>
                            <value>#CAG</value>
                            <value>#CTS</value>
                            <value>#other</value>
                        </choice>
                    </attribute>
                    <attribute name="cRef">
                        <choice>
                                <ref name="Bekker-lines-seperated-by-space"/>
                                <ref name="CAG-lines"/>
                                <ref name="printed-lines"/>
                                <ref name="CTS-URN"/>
                        </choice>
                    </attribute>
                </element>
            </optional>
        </element>
    </define>
    <define name="ref">
        <element name="ref">
            <!--optional>
                <attribute name="type"></attribute>
                <attribute name="cRef"></attribute>
            </optional-->
            <optional><attribute name="target" /></optional>
            <text></text>
        </element>
    </define>
    <define name="rs">
        <element name="rs">
            <attribute name="type">
                <choice>
                    <value>term</value>
                    <value>place</value>
                </choice>
            </attribute>
            <optional>
                <attribute name="key"/>
            </optional>
            <text></text>
        </element>
    </define>
    <define name="subst">
        <element name="subst">
            <optional><attribute name="resp"/></optional>
            <element name="del">
                <attribute name="rendition"/>
                <ref name="inlineElements"/>
            </element>
            <element name="add">
                <attribute name="place"/>
                <ref name="inlineElements"/>
            </element>
        </element>
    </define>
    <define name="supplied">
        <element name="supplied">
            <optional>
                <attribute name="cert"></attribute>
            </optional>
            <text/>
        </element>
    </define>
    <define name="surplus">
        <element name="surplus">
            <ref name="inlineElements"></ref>
        </element>
    </define>
    <define name="unclear">
        <element name="unclear">
            <attribute name="reason"/>
            <attribute name="cert" />
            <ref name="inlineElements"></ref>
        </element>
    </define>
    <!-- APPARATELEMENTE ######################################## -->
    <define name="anmerkung">
        <element name="seg">
            <attribute name="type"/>
            <element name="orig">
                <text/>
            </element>
            <element name="note">
                <attribute name="xml:id">
                    <data type="ID"/>
                </attribute>
                <text/>
            </element>
        </element>
    </define>
    <!-- VERWEISZEICHEN-->
    <define name="verweiszeichen">
        <element name="metamark">
                <attribute name="xml:id">
                    <data type="ID"/>
                </attribute>
                <attribute name="function">
                    <value>refSign</value>
                </attribute>
                <attribute name="place">
                    <choice>
                        <value>superlinear</value>
                        <value>sublinear</value>
                        <value>intralinear</value>
                    </choice>
                </attribute>
                <optional>
                    <attribute name="ed"/>
                </optional>
            </element>
    </define>
    <define name="Bekker-lines-seperated-by-space">
        <data type="string">
            <param name="pattern">(\d?\d?\d?\d[ab]\d\d\s)*\d?\d?\d?\d[ab]\d\d</param>
        </data>
    </define>
    <define name="CAG-lines">
        <data type="string">
            <param name="pattern">(\d+(,\d)?):(\d+)\.(\d+)(–\d+\.\d+)?</param>
        </data>
    </define>
    <define name="printed-lines">
        <data type="string">
            <param name="pattern">(\d+)\.(\d+)(–\d+\.\d+)?</param>
        </data>
    </define>
    <define name="CTS-URN">
        <data type="string">
            <param name="pattern">urn:cts:.+</param>
        </data>
    </define>
    <define name="diagramm-node">
        <element name="node">
            <attribute name="xml:id">
                <text/>
            </attribute>
            <attribute name="corresp">
                <text/>
            </attribute>
            <attribute name="ana">
                <choice>   
                    <value>unterbegriff</value>
                    <value>mittelbegriff</value>
                    <value>oberbegriff</value>
                    <value>mittelProsyllogismus-b</value>
                    <value>mittelProsyllogismus-c</value>
                    <value>mittelProsyllogismus-d</value>
                    <value>unklar</value>
                    <value>synemmenon</value>
                    <value>metalepsis</value>
                    <value>hypothetische-conclusion</value>
                </choice>
            </attribute>
            <oneOrMore>
                <element name="label">
                    <ref name="inlineElements"></ref>
                </element>
            </oneOrMore>
        </element>
    </define>
    <define name="diagramm-arc">
        <element name="arc">
            <attribute name="from">
                <text/>
            </attribute>
            <attribute name="to">
                <text/>
            </attribute>
            <attribute name="corresp">
                <text/>
            </attribute>
            <attribute name="ana">
                <choice>
                    <value>untersatz</value>
                    <value>obersatz</value>
                    <value>arist-conclusion</value>
                    <value>prosyll-b-1</value>
                    <value>prosyll-b-2</value>
                    <value>prosyll-c-1</value>
                    <value>prosyll-c-2</value>
                    <value>prosyll-d-1</value>
                    <value>prosyll-d-2</value>
                    <value>unklar</value>
                    <value>keineAussage</value>
                </choice>
            </attribute>
            <oneOrMore>
                <element name="label">
                    <attribute name="ana">
                        <choice>
                            <value>SaP</value>
                            <value>SeP</value>
                            <value>SiP</value>
                            <value>SoP</value>
                            <value>none</value>
                            <value>other</value>
                        </choice>
                    </attribute>
                    <ref name="inlineElements"></ref>
                </element>
            </oneOrMore>
            <optional>
                    <element name="figure">
                        <choice>
                            <element name="app">
                                <element name="lem">
                                    <attribute name="resp"></attribute>
                                    <attribute name="cert">
                                        <choice>
                                            <value>high</value>
                                            <value>medium</value>
                                            <value>low</value>
                                        </choice>
                                    </attribute>
                                    <ref name="syllogismus-diagramm"></ref>
                                </element>
                                <element name="rdg">
                                    <attribute name="wit"></attribute>
                                    <ref name="syllogismus-diagramm"></ref>
                                </element>
                            </element>
                            <ref name="syllogismus-diagramm"></ref>
                        </choice>
                    </element>
            </optional>
        </element>
    </define>
    <define name="syllogismus-diagramm">
        <element name="graph">
            <attribute name="type">
                <value>directed</value>
            </attribute>
            <attribute name="corresp">
                    <text/>
            </attribute>
            <attribute name="ana">
                <choice>
                    <value>1-barbara</value>
                    <value>1-celarent</value>
                    <value>1-darii</value>
                    <value>1-ferio</value>
                    <value>2-baroco</value>
                    <value>2-cesare</value>
                    <value>2-camestres</value>
                    <value>2-festino</value>
                    <value>3-bocardo</value>
                    <value>3-datisi</value>
                    <value>3-disamis</value>
                    <value>3-ferison</value>
                    <value>3-darapti</value>
                    <value>3-felapton</value>
                    <value>unklar</value>
                    <value>hypothetisch</value>
                    <value>falsch</value>
                </choice>
            </attribute>
            <optional>
                <attribute name="facs">
                    <text/>
                </attribute>
            </optional>
            <optional>
                <attribute name="subtype">
                    <value>beweis</value>
                </attribute>
            </optional>
            <oneOrMore>
                <ref name="diagramm-node"/>
            </oneOrMore>
            <zeroOrMore>
                <ref name="diagramm-arc"/>
            </zeroOrMore>
            <zeroOrMore>
                <element name="supplied">
                    <attribute name="cert">
                        <choice>
                            <value>high</value>
                            <value>medium</value>
                            <value>low</value>
                        </choice>
                    </attribute>
                    <ref name="diagramm-arc"/>
                </element>
            </zeroOrMore>
            <zeroOrMore>
                <ref name="diagramm-arc"/>
            </zeroOrMore>
        </element>
    </define>
    <define name="respStmt">
        <element name="respStmt">
            <element name="persName">
                <attribute name="ref"></attribute>
                <element name="surname">
                    <text/>
                </element>
                <element name="forename">
                    <text/>
                </element>
            </element>
            <optional>
                <element name="orgName">
                    <attribute name="ref"></attribute>
                    <text/>
                </element>
            </optional>
            <oneOrMore>
                <ref name="resp"/>
            </oneOrMore>
        </element>
    </define>
    <define name="resp">
        <element name="resp">
            <attribute name="ref">
                <choice>
                    <value>https://credit.niso.org/contributor-roles/conceptualization/</value>
                    <value>https://credit.niso.org/contributor-roles/data-curation/</value>
                    <value>https://credit.niso.org/contributor-roles/formal-analysis/</value>
                    <value>https://credit.niso.org/contributor-roles/funding-acquisition/</value>
                    <value>https://credit.niso.org/contributor-roles/investigation/</value>
                    <value>https://credit.niso.org/contributor-roles/methodology/</value>
                    <value>https://credit.niso.org/contributor-roles/project-administration/</value>
                    <value>https://credit.niso.org/contributor-roles/software/</value>
                    <value>https://credit.niso.org/contributor-roles/resources/</value>
                    <value>https://credit.niso.org/contributor-roles/supervision/</value>
                    <value>https://credit.niso.org/contributor-roles/visualization/</value>
                    <value>https://credit.niso.org/contributor-roles/validation/</value>
                    <value>https://credit.niso.org/contributor-roles/writing-original-draft/</value>
                    <value>https://credit.niso.org/contributor-roles/writing-review-editing/</value>
                </choice>
            </attribute>
            <attribute name="from">
                <choice>
                    <ref name="YYYY"/>
                    <ref name="YYYY-MM"/>
                    <ref name="YYYY-MM-DD"/>
                </choice>
            </attribute>
            <attribute name="to">
                <choice>
                    <ref name="YYYY"/>
                    <ref name="YYYY-MM"/>
                    <ref name="YYYY-MM-DD"/>
                </choice>
            </attribute>
            <text/>
        </element>
    </define>
    <define name="encodingDesc">
        <element name="encodingDesc">
            <oneOrMore>
                <ref name="refsDecl"/>
            </oneOrMore>
            <ref name="editorialDecl"/>
            <element name="schemaRef">
                <attribute name="url"></attribute>
            </element>
        </element>
    </define>
    <define name="refsDecl">
        <element name="refsDecl">
            <attribute name="xml:id"></attribute>
            <element name="cRefPattern">
                <attribute name="matchPattern"></attribute>
                <attribute name="replacementPattern"></attribute>
                <optional>
                    <element name="p">
                        <text/>
                    </element>
                </optional>
            </element>
        </element>
    </define>
    <define name="editorialDecl">
        <element name="editorialDecl">
            <zeroOrMore>
                <element name="interpretation">
                    <attribute name="n"></attribute>
                    <attribute name="method">
                        <value>markup</value>
                    </attribute>
                    <element name="p">
                        <interleave>
                            <zeroOrMore>
                            <element name="gi">
                                <text/>
                            </element>
                            </zeroOrMore>
                            <zeroOrMore>
                                <element name="att">
                                    <text/>
                                </element>
                            </zeroOrMore>
                            <text/>
                        </interleave>
                    </element>
                </element>
            </zeroOrMore>
            <optional>
                <element name="correction">
                    <attribute name="method">
                        <choice>
                            <value>markup</value>
                            <value>silent</value>
                        </choice>
                    </attribute>
                    <element name="p">
                        <interleave>
                            <zeroOrMore>
                            <element name="gi">
                                <text/>
                            </element>
                            </zeroOrMore>
                            <zeroOrMore>
                                <element name="att">
                                    <text/>
                                </element>
                            </zeroOrMore>
                            <text/>
                        </interleave>
                    </element>
                </element>
            </optional>
            <zeroOrMore>
                <element name="normalization">
                    <attribute name="n"></attribute>
                    <attribute name="method">
                        <choice>
                            <value>markup</value>
                            <value>silent</value>
                        </choice>
                    </attribute>
                    <element name="p">
                        <interleave>
                            <zeroOrMore>
                            <element name="gi">
                                <text/>
                            </element>
                            </zeroOrMore>
                            <zeroOrMore>
                                <element name="att">
                                    <text/>
                                </element>
                            </zeroOrMore>
                            <text/>
                        </interleave>
                    </element>
                </element>
            </zeroOrMore>
            <optional>
                <element name="punctuation">
                    <attribute name="marks">
                        <choice>
                            <value>none</value>
                            <value>some</value>
                            <value>all</value>
                        </choice>
                    </attribute>
                    <element name="p">
                        <interleave>
                            <zeroOrMore>
                            <element name="gi">
                                <text/>
                            </element>
                            </zeroOrMore>
                            <zeroOrMore>
                                <element name="att">
                                    <text/>
                                </element>
                            </zeroOrMore>
                            <text/>
                        </interleave>
                    </element>
                </element>
            </optional>
        </element>
    </define>
</grammar>
